        <servlet-name>AcsAP</servlet-name>
        <servlet-class>AcsAP</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AcsAM</servlet-name>
        <servlet-class>AcsAM</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AcsTel</servlet-name>
        <servlet-class>AcsTel</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AcTodo</servlet-name>
        <servlet-class>AcTodo</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ConsultarAlumnos</servlet-name>
        <url-pattern>/ConsultarAlumnos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistrarAlumnos</servlet-name>
